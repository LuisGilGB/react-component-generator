{
  "name": "@luisgilgb/react-component-generator",
  "version": "0.1.1",
  "description": "CLI tool that generates the scaffolding for a React component library",
  "main": "cli.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "rm-modules": "rm -rf ./node_modules",
    "clean-install": "npm run rm-modules && npm install",
    "clean-install-pro": "npm run rm-modules && npm install --production",
    "publish-pro": "npm run clean-install-pro && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuisGilGB/react-component-generator.git"
  },
  "keywords": [
    "React",
    "component",
    "generator",
    "CLI"
  ],
  "author": "Luis Gil Guti√©rrez de la Barreda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LuisGilGB/react-component-generator/issues"
  },
  "homepage": "https://github.com/LuisGilGB/react-component-generator#readme",
  "devDependencies": {
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "@luisgilgb/js-utils": "^0.3.1",
    "chalk": "^3.0.0",
    "fs-extra": "^8.1.0",
    "yargs": "^15.0.2"
  }
}
